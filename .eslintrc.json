{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "no-undef": "off",
    "no-bitwise": "off",
    "prefer-const": "off",
    "no-plusplus": "off",
    "max-class"
  }
}
